# Firebase Configuration Template
# Copy this file to .env and fill in your actual Firebase credentials
# NEVER commit .env to version control (it's in .gitignore)

# WEB
WEB_API_KEY=your_web_api_key
WEB_APP_ID=your_web_app_id

# ANDROID
ANDROID_API_KEY=your_android_api_key
ANDROID_APP_ID=your_android_app_id

# IOS & MACOS
IOS_API_KEY=your_ios_api_key
IOS_APP_ID=your_ios_app_id

# WINDOWS
WINDOWS_API_KEY=your_windows_api_key
WINDOWS_APP_ID=your_windows_app_id

# COMMON
MESSAGING_SENDER_ID=your_messaging_sender_id
PROJECT_ID=your_project_id
AUTH_DOMAIN=your_project_id.firebaseapp.com
DATABASE_URL=https://your_project_id-default-rtdb.firebaseio.com
STORAGE_BUCKET=your_project_id.firebasestorage.app
MEASUREMENT_ID=your_measurement_id
WINDOWS_MEASUREMENT_ID=your_windows_measurement_id
IOS_BUNDLE_ID=your.bundle.id
